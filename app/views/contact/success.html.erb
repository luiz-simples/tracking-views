<div class="blog-header">
  <h1 class="blog-title">Success#Page</h1>
  <p class="lead blog-description">Find me in app/views/contact/success.html.erb</p>
</div>

<% if flash[:notice] %>
   <div class="alert alert-success">
     <h2 class="error"><%= flash[:notice] %></h2>
  </div>
<% end %>

<div class="row">
  <div class="col-sm-8 blog-main">
    <%= render template: "layouts/form" %>
    <%= render template: "layouts/post" %>
  </div>
</div>
